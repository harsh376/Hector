{
  "parser": "babel-eslint",
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "expect": false,
    "jest": false,
    "document": false,
    "window": false,
    "Headers": false,
  },
  "rules": {
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "prefer-stateless-function": [0, { "ignorePureComponents": true }]
  }
}

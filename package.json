{
  "name": "client",
  "version": "1.0.0",
  "description": "React app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "bundle:dev": "node_modules/webpack/bin/webpack.js --config webpack/webpack.config.dev.js",
    "bundle:prod": "node_modules/webpack/bin/webpack.js --config webpack/webpack.config.prod.js",
    "build:server": "babel src/server --out-dir dist/server --copy-files",
    "build:dev": "yarn run clean && yarn run bundle:dev && yarn run build:server",
    "build:prod": "yarn run clean && yarn run bundle:prod && yarn run build:server",
    "deploy": "yarn run build:prod && NODE_ENV=production PORT=4000 USER_AUTH=disabled node dist/server/server.js",
    "heroku-postbuild": "yarn run build:prod",
    "deploy:heroku": "NODE_ENV=production USER_AUTH=disabled node dist/server/server.js",
    "start": "npm rebuild node-sass && USER_AUTH=enabled babel-node dist/server/server.js",
    "start:noauth": "npm rebuild node-sass && USER_AUTH=disabled babel-node dist/server/server.js",
    "test": "yarn run lint && yarn run jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "mocha": "mocha ./test_setup.js 'src/**/**/*.unit.@(js)'",
    "jest": "node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/harsh376/Hector.git"
  },
